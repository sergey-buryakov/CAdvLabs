#include <vector>
#include <iostream>

using namespace std;
int main()
{
	vector <float> v(5);
	float   number,
	        sum = 0,
	        avg = 0;
	cin >> number;
    v[0] = number;
    v[0] = number + 5);
    v[0] = number + 0.5);
    v[0] = number + 2.3);
    v.push_back(number + 4.7);

    for (int i = 0; i < v.size(); i++)
    {
        sum += v[i];
    }
    avg = sum / v.size();
    cout << "Sum: " << sum << ", avg: " << avg << endl;
	return 0;
}
